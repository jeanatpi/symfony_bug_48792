{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-sodium": "*",
        "alek13/slack": "^2.2",
        "beberlei/assert": "^3.3",
        "beberlei/doctrineextensions": "^1.3",
        "bysolutions/apaleo-client": "^1.1",
        "devio/pipedrive": "^2.12",
        "doctrine/dbal": "^3.1",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.10",
        "firebase/php-jwt": "^6.2",
        "guzzlehttp/guzzle": "^7.4",
        "knplabs/knp-snappy-bundle": "^1.9",
        "knplabs/knp-time-bundle": "^1.19",
        "knpuniversity/oauth2-client-bundle": "^2.10",
        "laminas/laminas-code": "^4.6",
        "laminas/laminas-eventmanager": "^3.5",
        "league/csv": "^9.8.0",
        "myclabs/deep-copy": "^1.11",
        "nbgrp/onelogin-saml-bundle": "^1.1",
        "paragonie/halite": "^5.1",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpseclib/phpseclib": "^3.0.14",
        "phpstan/phpdoc-parser": "^1.7",
        "phpxmlrpc/phpxmlrpc": "^4.9",
        "predis/predis": "^2.0",
        "ramsey/uuid": "^4.3",
        "scienta/doctrine-json-functions": "^5.0",
        "smi2/phpclickhouse": "^1.4",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "6.1.*",
        "symfony/console": "6.1.*",
        "symfony/doctrine-messenger": "6.1.*",
        "symfony/dotenv": "6.1.*",
        "symfony/expression-language": "6.1.*",
        "symfony/finder": "6.1.*",
        "symfony/flex": "^2.0",
        "symfony/form": "6.1.*",
        "symfony/framework-bundle": "6.1.*",
        "symfony/http-client": "6.1.*",
        "symfony/http-kernel": "6.1.*",
        "symfony/intl": "6.1.*",
        "symfony/lock": "6.1.*",
        "symfony/mailer": "6.1.*",
        "symfony/messenger": "6.1.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/property-access": "6.1.*",
        "symfony/property-info": "6.1.*",
        "symfony/proxy-manager-bridge": "6.1.*",
        "symfony/rate-limiter": "6.1.*",
        "symfony/runtime": "6.1.*",
        "symfony/security-bundle": "6.1.*",
        "symfony/serializer": "6.1.*",
        "symfony/service-contracts": "^1.1",
        "symfony/translation": "*",
        "symfony/twig-bundle": "*",
        "symfony/uid": "6.1.*",
        "symfony/validator": "*",
        "symfony/web-link": "6.1.*",
        "symfony/yaml": "6.1.*",
        "twig/extra-bundle": "^3.4",
        "twig/intl-extra": "^3.4",
        "twig/twig": "^3.4",
        "willdurand/js-translation-bundle": "^5.0",
        "wwsh/symfony-listjs": "dev-master",
        "zircote/swagger-php": "^4.4"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"

    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.1.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsofphp/php-cs-fixer": "^3.8",
        "phpstan/phpstan": "^1.9",
        "rector/rector": "^0.15.2",
        "sidus/doctrine-debug-bundle": "^1.0",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/browser-kit": "6.1.*",
        "symfony/css-selector": "6.1.*",
        "symfony/maker-bundle": "^1.12",
        "symfony/phpunit-bridge": "6.1.*",
        "symfony/stopwatch": "6.1.*",
        "symfony/var-dumper": "6.1.*",
        "symfony/web-profiler-bundle": "6.1.*"
    }
}
